{   
    "task": {
        "name": "Fed_Seg",
        "type": "seg"
    },
    "model": {
        "name": "unet",
        "n_channels": 3,
        "n_classes": 4
    },
    "train": {
        "epochs": 5,
        "batch_size": 2,
        "criterion": "cross_entropy",
        "write_tensorboard": true,
        "save_best_model": true,
        "seed": 2020
    },
    "optimizer": {
        "name": "rms_prop",
        "lr": 1e-4,
        "weight_decay": 5e-4,
        "momentum": 0.9
    }
}