{   
    "model": {
        "n_channels": 3,
        "n_classes": 4
    },
    "train": {
        "epochs": 1,
        "batch_size": 2
    },
    "data": {
        "data_root": "raw_data/ct_lesion_seg",
        "image_resize_shape": [224, 224]
    },
    "optimizer": {
        "lr": 1e-4,
        "weight_decay": 5e-4,
        "momentum": 0.9
    },
    "federate": {
        "round": 50,
        "client_num": 2
    }
}